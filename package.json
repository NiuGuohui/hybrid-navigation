{
  "name": "hybrid-navigation",
  "description": "React Native Navigation that supports seamless navigation between Native and React.",
  "version": "2.7.2",
  "keywords": [
    "react-native",
    "navigation",
    "navigator",
    "router",
    "hybrid"
  ],
  "nativePackage": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/listenzz/hybrid-navigation.git"
  },
  "homepage": "https://github.com/listenzz/hybrid-navigation",
  "author": "listen <listenzz@163.com>",
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "tsc": "tsc",
    "start": "react-native start --reset-cache",
    "run:ios": "react-native run-ios --project-path ./example/ios",
    "start:android": "adb shell am start -n com.reactnative.hybridnavigation.example/.MainActivity",
    "run:android": "cd example/android && ./gradlew installDebug && npm run start:android",
    "test": "jest",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-native": ">=0.60"
  },
  "dependencies": {
    "path-to-regexp": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@gfez/eslint-config-react-native": "^1.0.0",
    "@react-native-community/hooks": "^2.8.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.1",
    "@types/react-native": "^0.66.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "17.0.1",
    "babel-jest": "^27.2.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^27.2.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react": "17.0.2",
    "react-native": "^0.66.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-iphone-x-helper": "^1.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-toast-hybrid": "^2.4.4",
    "react-redux": "^7.2.5",
    "react-test-renderer": "17.0.2",
    "redux": "^4.1.1",
    "typescript": "^4.4.3",
    "vuepress": "^2.0.0-beta.26"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
